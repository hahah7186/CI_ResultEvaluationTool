	<GlobalTestsStatus>
		<Failure>
		 //TestFixtureResult/Result[text()='Failure']
		</Failure>
		<TimeStamp>
		 //TestFixtureResult/ExecutionDate
		</TimeStamp>	
	</GlobalTestsStatus>
	<SingleTestStatus>
		<Test>
		 //PlcTag
		</Test>
		<Failure></Failure>
		<Success></Suc
